{
  "name": "scrapper",
  "version": "1.0.0",
  "description": "Proyecto para extraer la información de `Mini Pc's` de diferentes páginas web",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --outDir dist",
    "start": "ts-node src/index.ts",
    "schema": "typescript-json-schema src/miniPcExtractedData.ts MiniPcExtractedData --required --out data/schema.json",
    "database": "npx supabase gen types typescript --project-id \"whjfsusmhvfefxhxgzch\" --schema public > src/database.types.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@gfs-studio/webbutler-js": "^1.0.16",
    "@supabase/supabase-js": "^2.49.4",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^16.5.0",
    "jsdom": "^26.1.0",
    "openai": "^4.89.0",
    "typescript-json-schema": "^0.65.1"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
